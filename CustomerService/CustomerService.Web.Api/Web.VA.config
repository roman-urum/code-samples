<?xml version="1.0" encoding="utf-8"?>

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <connectionStrings>
    <add name="CustomerServiceDbContext" connectionString="Server=VA-SQL; Database=CustomerService; Trusted_Connection=True;" providerName="System.Data.SqlClient"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
    <add name="RedisCache" connectionString="localhost:6379" providerName="StackExchange.Redis"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
  </connectionStrings>

  <appSettings>
    <add key="ApiBaseUrl" value="http://customerservice.toodleday.net"
         xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="TokenServiceUrl" value="http://tokenservice.toodleday.net"
         xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
  </appSettings>

</configuration>