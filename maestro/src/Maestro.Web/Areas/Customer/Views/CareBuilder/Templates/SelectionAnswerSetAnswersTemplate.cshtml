<script id="selectionAnswerSetAnswersTemplate" type="text/template">
    <div class="panel-body">
        <div class="row">
            <div class="col-sm-2 text-right strong">
                Answers:
                <% if (isMultipleChoice) { %>
                    <br />@Html.Partial("_MultiSelectLabel")
                <% } %>
            </div>
            <div class="col-sm-10">
                <ul class="list-unstyled">
                    <% _.each(selectionAnswerChoices.toJSON(), function(answerChoice){ %>
                    <li><%=answerChoice.answerString.value%></li>
                    <% }); %>
                </ul>
            </div>
        </div>
    </div>
</script>