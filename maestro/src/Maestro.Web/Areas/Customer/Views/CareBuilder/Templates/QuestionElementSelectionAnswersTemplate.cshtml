<script id="questionElementSelectionAnswersTemplate" type="text/template">
    <h4>Multiple Choice: <%= name %></h4>
    <div class="row">
        <div class="col-sm-2 strong">
            Answers
            <% if (isMultipleChoice) { %>
                <br />@Html.Partial("_MultiSelectLabel")
            <% } %>
        </div>
        <div class="col-sm-10">
        <% _.each(selectionAnswerChoices.toJSON(), function(answerChoice, index){ %>
            <%= answerChoice.answerString.value %><br>
        <% }); %>
        </div>
    </div>
</script>