<script id="programListItemTemplate" type="text/template">
    <div class="panel panel-default">
        <div class="panel-heading clearfix" role="tab" id="heading-<%=id%>">
            <input type="hidden" value="<%= id %>" name="id" />
            <div class="row">
                <div class="col-sm-10">
                    <h4 class="panel-title strong">
                        <span><%= name %></span>
                    </h4>
                </div>
                <div class="col-sm-2 small italic text-right">Program</div>
            </div>
        </div>
        <div class="panel-footer clearfix">
            <div class="pull-left default-language">
                <strong>Default language:</strong> EN-US
            </div>
            <div class="tags-labels">
                <%_.each(tags, function (tag) { %>
                    <label class="tag"><%= tag %></label>
                <% });%>
            </div>
        </div>
    </div>
</script>